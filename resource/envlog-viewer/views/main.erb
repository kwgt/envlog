<html>
  <head>
    <meta charset="utf-8">
      <title>SENSOR LIST</title>

      <% if not $develop_mode %>
        <meta http-equiv="Pragma" content="no-cache">
        <meta http-equiv="Cache-Control" content="no-cache">
        <meta http-equiv="Expires" content="-1">
      <% end %>
    </meta>

    <script type="text/javascript" src="/js/jquery-3.5.1.min.js"></script>
    <script type="text/javascript" src="/js/const.js"></script>
    <script type="text/javascript" src="/js/util.js"></script>
    <script type="text/javascript" src="/js/main.js"></script>
  </head>

  <body class="main-screen" style="display:none;">
    <div>
      <table id="sensor-table" class="table">
        <thead class="thead-dark">
          <tr>
            <th class="sensor-num" scope="col">#</th>
            <th class="last-update" scope="col">UPDATE</th>
            <th class="temperature" scope="col">TEMP</th>
            <th class="humidity" scope="col">HUM</th>
            <th class="air-pressure" scope="col">A/P</th>
            <th class="vbat" scope="col">VBAT</th>
            <th class="vbus" scope="col">VBUS</th>
            <th class="state" scope="col">STATE</th>
            <th class="description" scope="col">DESCRIPTION</th>
            <th class="control" scope="col"></th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>

    <%= render :erb, :"../../common/views/abort_shield" %>
    <%= render :erb, :"../../common/views/loading_shield" %>
    <%= render :erb, :"../../common/views/error_modal" %>
    <%= render :erb, :"../../common/views/confirm_modal" %>
    <%= render :erb, :"misc/dialog/device_info" %>
  </body>
</html>

